{
  "name": "gulp-boilerplate",
  "version": "5.1.0",
  "description": "gulpfile.js template for web development",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build": "GIT_COMMIT_HASH=$(git rev-parse HEAD) gulp build",
    "build:production": "NODE_ENV=production GIT_COMMIT_HASH=$(git rev-parse HEAD) gulp build",
    "clean": "gulp clean",
    "lint:fix": "eslint src --fix",
    "stylelint:css": "stylelint --fix **/*.{css,scss,sass}"
  },
  "private": true,
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "@prettier/plugin-php": "^0.17.2",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.4",
    "del": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-ejs": "^5.1.0",
    "gulp-if": "^3.0.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "postcss": "^8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-stream": "^6.1.2"
  },
  "dependencies": {
    "jquery": "^3.6.0"
  },
  "peerDependencies": {
    "postcss": "^8.3.5"
  }
}
